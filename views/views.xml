<odoo>
  <data>
    <record id='travel_hr_expense_view_form' model="ir.ui.view">
        <field name="name">travel.hr.expense.view.form</field>
        <field name="model">hr.expense</field>
        <field name="inherit_id" ref='hr_expense.hr_expense_view_form'/>
        <field name="arch" type="xml">
            <xpath expr="/form/sheet/group[1]" position="after">
              <group>
                <field name="date_from"/>
                <field name="date_to"/>
                <field name="compute_time_diff"/>
                <field name="compute_day_diff"/>
                <field name="compute_remain_hours"/>
                <field name="compute_travel_base_price"/>
                <field name="free_food_num"/>
                <field name="total_travel"/>
                <field name="first_day_hours"/>
                <field name="last_day_hours"/>
                <field name="full_day_num"/>
              </group>
            </xpath>
        </field>
      </record>

